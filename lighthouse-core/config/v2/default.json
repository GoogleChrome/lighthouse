    },
    "no-document-write": {
      "path": "dobetterweb/no-document-write"
    },
    "no-mutation-events": {
      "path": "dobetterweb/no-mutation-events"
    },
    "no-old-flexbox": {
      "path": "dobetterweb/no-old-flexbox"
    },
    "no-websql": {
      "path": "dobetterweb/no-websql"
    },
    "notification-on-start": {
      "path": "dobetterweb/notification-on-start"
    },
