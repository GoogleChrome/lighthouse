    "manifest-exists": {},
    "manifest-background-color": {},
    "manifest-theme-color": {},
    "manifest-icons-min-192": {},
    "manifest-icons-min-144": {},
    "manifest-name": {},
    "manifest-short-name": {},
    "manifest-short-name-length": {},
    "manifest-start-url": {},
    "theme-color-meta": {},
    "content-width": {},
    "deprecations": {},
    "aria-allowed-attr": {
      "path": "accessibility/aria-allowed-attr"
    },
    "aria-required-attr": {
      "path": "accessibility/aria-required-attr"
